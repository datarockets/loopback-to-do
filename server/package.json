{
  "name": "server",
  "version": "0.1.0",
  "description": "ToDo app API",
  "main": "build",
  "private": true,
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src\"",
    "dev-test": "NODE_ENV=test PORT=8000 yarn dev",
    "debug": "nodemon -w src src/index.js --exec 'babel-node --inspect'",
    "build": "babel src -s -D -d build",
    "start": "node build",
    "lint": "eslint src",
    "test": "NODE_ENV=test yarn jasmine",
    "test-api": "NODE_ENV=test babel-node node_modules/.bin/dredd"
  },
  "dependencies": {
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "dotenv": "^6.1.0",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "pg": "^7.6.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.41.2",
    "sequelize-cli": "^5.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.0.0-0",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "babel-eslint": "^9.0.0",
    "dredd": "^5.3.0",
    "eslint": "5.6.0",
    "eslint-config-datarockets-base": "^0.0.4-6",
    "eslint-plugin-import": "^2.13.0",
    "jasmine": "^3.3.0",
    "nodemon": "^1.9.2",
    "request": "^2.88.0"
  }
}
